{
  "name": "vite-vue-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc && vite build --mode test",
    "build:pre": "vue-tsc && vite build --mode staging",
    "build:dev": "vue-tsc && vite build --mode development",
    "dev": "pnpm vite",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier": "prettier -c --write **/*",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "lint": "npm run tsc && npm run lint:demo && npm run lint:md && npm run lint:script && npm run lint:site",
    "lint:components": "eslint --fix --ext .jsx,.js,.ts,.tsx ./components",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:site": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.ts,.tsx,vue ./site",
    "lint:style": "stylelint \"{site,components}/**/*.scss\" --syntax scss",
    "myLint": "./node_modules/.bin/eslint --fix --ext .js,.jsx,.ts,.tsx ."
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{css,scss,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    },
    "wa-menus": {
      "xlsxDefaultLan": "zh-CN",
      "xlsxTransformPath": "src/locales",
      "xlsxTransformType": "ts"
    }
  },
  "dependencies": {
    "@types/vue": "^2.0.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "next",
    "add": "^2.0.6",
    "ant-design-vue": "^4.0.0",
    "axios": "^1.6.5",
    "babel-eslint": "^10.1.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "eslint-plugin-import": "^2.25.2",
    "mockjs": "^1.1.0",
    "path": "^0.12.7",
    "postcss": "^8.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "smooth-scrollbar": "^8.8.4",
    "url": "^0.11.1",
    "vite-plugin-mkcert": "^1.16.0",
    "vue": "^3.3.4",
    "vue-hooks-plus": "^1.8.6",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.4",
    "vuex": "^4.1.0",
    "wa-utils": "^0.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "*",
    "vite": "^4.4.7",
    "vite-plugin-mock": "^2.9.6",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.5",
    "wa-postcss-pxtorem": "^0.0.1"
  }
}
